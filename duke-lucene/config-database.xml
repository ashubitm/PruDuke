<duke>

  <schema>
    <threshold>0.49</threshold>

    <property>
      <name>FIRSTNAME</name>
      <comparator>no.priv.garshol.duke.comparators.Levenshtein</comparator>
      <low>0.48</low>
      <high>0.8</high>
    </property>

    <property>
      <name>LASTNAME</name>
       <comparator>no.priv.garshol.duke.comparators.Levenshtein</comparator>
      <low>0.3</low>
      <high>0.8</high>
    </property> 
    
  </schema>
  
  
  
  <data-source class="no.priv.garshol.duke.datasources.InMemoryDataSource">
  <param name="input-file" value="Whatever.json"/>
  </data-source> 
  
 
